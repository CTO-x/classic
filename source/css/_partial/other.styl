@media only screen and (min-width: 550px)
  .maskImage
    body
      & > nav
        ul[data-subscription$=email]
          & + form
            width 123px
@media only screen and (min-width: 992px)
  .maskImage
    body
      & > nav
        ul[data-subscription$=email]
          & + form
            width 173px
.maskImage
  ul
    &.subscription
      position relative
  a[rel=subscribe-rss]
    position relative
    top 0px
    text-indent -999999em
    background-color #dedede
    border 0
    padding 0
  a[rel=subscribe-rss]:after
    content ""
    position absolute
    top -1px
    left 0
    background-color #ababab
  a[rel=subscribe-rss]:hover
    &:after
      background-color #9e9e9e
  a[rel=subscribe-email]
    position relative
    top 0px
    text-indent -999999em
    background-color #dedede
    border 0
    padding 0
  a[rel=subscribe-email]:after
    content ""
    position absolute
    top -1px
    left 0
    background-color #ababab
  a[rel=subscribe-email]:hover
    &:after
      background-color #9e9e9e
.maskImage ul.subscription li,.maskImage ul.subscription a
  border 0
  padding 0
.maskImage a[rel=subscribe-rss],.maskImage a[rel=subscribe-rss]:after
  -webkit-mask-image url('/images/rss.png?1425807049')
  -moz-mask-image url('/images/rss.png?1425807049')
  -ms-mask-image url('/images/rss.png?1425807049')
  -o-mask-image url('/images/rss.png?1425807049')
  mask-image url('/images/rss.png?1425807049')
  -webkit-mask-repeat no-repeat
  -moz-mask-repeat no-repeat
  -ms-mask-repeat no-repeat
  -o-mask-repeat no-repeat
  mask-repeat no-repeat
  width 22px
  height 22px
.maskImage a[rel=subscribe-email],.maskImage a[rel=subscribe-email]:after
  -webkit-mask-image url('/images/email.png?1425807048')
  -moz-mask-image url('/images/email.png?1425807048')
  -ms-mask-image url('/images/email.png?1425807048')
  -o-mask-image url('/images/email.png?1425807048')
  mask-image url('/images/email.png?1425807048')
  -webkit-mask-repeat no-repeat
  -moz-mask-repeat no-repeat
  -ms-mask-repeat no-repeat
  -o-mask-repeat no-repeat
  mask-repeat no-repeat
  width 28px
  height 22px
.googleplus
  h1
    -moz-box-shadow none !important
    -webkit-box-shadow none !important
    -o-box-shadow none !important
    box-shadow none !important
    border-bottom 0px none !important
  a
    text-decoration none
    white-space normal !important
    line-height 32px
    img
      float left
      margin-right 0.5em
      border 0 none
.googleplus-hidden
  position absolute
  top -1000em
  left -1000em
#pinboard_linkroll .pin-title,#pinboard_linkroll .pin-description
  display block
  margin-bottom .5em
#pinboard_linkroll
  .pin-tag
    text-decoration none
    &:after
      content ','
    &:last-child
      &:after
        content ''
/*#pinboard_linkroll .pin-tag:hover{text-decoration:underline}*/
.delicious-posts
  a
    &.delicious-link
      margin-bottom .5em
      display block
  p
    font-size 1em
